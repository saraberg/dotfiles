
!!!! URxvt !!!!
!URxvt.background:		#000000
!URxvt.foreground:		#2b6a99
!URxvt.cursorColor:		#2b6a99
URxvt.font:				xft:Noto Mono-10:antialias=true
!URxvt.font:				xft:fixed-9:antialias=true

URxvt.scrollBar:		false
URxvt.perl-ext-common:	default,matcher,tabbed
URxvt.urlLauncher:		open-url.pl
URxvt.matcher.button:	3
URxvt.saveLines:		8192
URxvt.termName:			xterm-256color
URxvt.urgentOnBell: true
URxvt.visualBell: true
URxvt.print-pipe: ""


!!! Tabbed colors !!!
URxvt.tabbed.tabbar-fg: 2
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg: 3
URxvt.tabbed.tab-bg: 0

!!!! Xft !!!!
Xft.dpi:       96
Xft.antialias: 1
Xft.hinting:   1
Xft.autohint: 0
Xft.hintstyle: hintslight
Xft.rgba:      rgb
Xft.lcdfilter: lcddefault

!!!! xosview
xosview*background:			black
xosview*borderwidth:		0

/* 
 *      default terminal colors 
 * 
 * 0-7 low-intensity
 * 8-15 high-intensity */

! black
!*color0:  #2e3436  
!*color8:  #555753 
!*color0:  COLOR_TERM_BLACK
!*color8:  COLOR_TERM_DARK_GRAY

! red
!*color1:  #cc0000 
!*color9:  #ef2929 
!*color1:  COLOR_TERM_DARK_RED 
!*color9:  COLOR_TERM_LIGHT_RED

! green
!*color2:  #00ff00 
!*color10: #66ff66 
!*color2:  COLOR_TERM_DARK_GREEN 
!*color10: COLOR_TERM_LIGHT_GREEN

! yellow
!*color3:  #c4a000 
!*color11: #fc394f 
!*color3:  COLOR_TERM_DARK_YELLOW 
!*color11: COLOR_TERM_LIGHT_YELLOW

! blue
!*color4:  #3456a4
!*color12: #729fcf 
!*color4:  COLOR_TERM_DARK_BLUE
!*color12: COLOR_TERM_LIGHT_BLUE

! magenta
!*color5:  #75507b
!*color13: #ad7fa8 
!*color5:  COLOR_TERM_DARK_MAGENTA
!*color13: COLOR_TERM_LIGHT_MAGENTA 

! orange (default cyan)
!*color6:  #418179
!*color14: #34e2e2
!*color6:  COLOR_TERM_DARK_CYAN
!*color14: COLOR_TERM_LIGHT_CYAN

! white
!*color7:  #d3d7cf 
!*color15: #eeeeec 
!*color7:  COLOR_TERM_DARK_GRAY
!*color15: COLOR_TERM_WHITE

